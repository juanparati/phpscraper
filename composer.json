    "league/climate": "^3.5",
    "symfony/dom-crawler": "^4.3",
    "symfony/yaml": "^4.3",
    "amphp/artax": "^3.0",
    "ext-dom": "*"
  },
  "bin": [
    "build/phpscraper"
  ]
}
