{
  "name": "juanparati/phpscraper",
  "description": "A PHP scraper",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Lago",
      "email": "juanparati@gmail.comm"
    }
  ],
  "config": {
    "vendor-dir": "src/Vendor"
  },
  "scripts": {
    "post-create-project-cmd": [
      "@composer create-project mamuph/system src/System \"2.2\""
